    },
    "android": {
      "package": "com.timsa7.mundmobile",
      "versionCode": 3,
      "permissions": [],
      "config": {
        "googleMobileAdsAppId": "ca-app-pub-3940256099942544~3347511713"
      }
    },
    "entryPoint": "node_modules/expo/AppEntry.js"
  }
